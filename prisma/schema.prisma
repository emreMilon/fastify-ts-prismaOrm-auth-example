generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  userId String @unique
  firstName String 
  lastName String
  position Position @default(Vertrieber) 
  email String @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Position {
  Leiter
  Vertrieber
}